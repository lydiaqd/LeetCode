/**
 * @param {string} s
 * @return {boolean}
 */
var isPalindrome = function(s) {
    let lowerCase = s.toLowerCase();
    let removeSpecial = lowerCase.replace(/[^a-zA-Z0-9]/g, '');
    let newString = removeSpecial.split("");
    let reversedString = newString.reverse();
    let reversedWord = reversedString.join("");

    console.log(reversedWord)

    if (reversedWord === "") {return true
    } else if (reversedWord == s) {
        return true
    } else {
        return false
    }
};

//String to array
//Reverse original array and add to new array
//Compare original array with new array